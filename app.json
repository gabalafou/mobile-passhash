{
  "expo": {
    "name": "Mobile Passhash",
    "description": "Simple, effective password security",
    "githubUrl": "https://github.com/gabalafou/mobile-passhash",
    "icon": "src/icon.png",
    "slug": "mobile-passhash",
    "privacy": "public",
    "version": "1.1.0",
    "entryPoint": "node_modules/expo/AppEntry.js",
    "splash": {
      "image": "src/assets/splash.png"
    },
    "platforms": ["ios", "android", "web"],
    "ios": {
      "bundleIdentifier": "com.gabalafou.mobilepasshash",
      "buildNumber": "1.1.0.2",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.gabalafou.mobilepasshash",
      "versionCode": 2,
      "permissions": []
    },
    "extra": {
      "debugLogging": false
    }
  }
}
